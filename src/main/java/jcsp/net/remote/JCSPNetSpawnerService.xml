<JCSP-CONFIG>
	<PLUGINS>
		<PLUGIN name="UIFactory" classname="jcsp.net.UIFactory"/>
	</PLUGINS>
    <SERVICES>
        <SERVICE name="dynamic_class_loading" class="jcsp.net.dynamic.DynamicClassLoader" run="TRUE"/>
    </SERVICES>
	<PROTOCOLS>
        <PROTOCOL id="TCPIP" name="TCP/IP" idclass="jcsp.net.tcpip.TCPIPProtocolID">
			<SPECS>
				<MAXSPEED value="9500000"/>
				<WIRELESS value="FALSE" />
				<RELIABLE value="TRUE"/>
				<CONNECTION-ORIENTED value="TRUE"/>
			</SPECS>
		</PROTOCOL>
	</PROTOCOLS>
	<ADDRESSES>
        <ADDRESS protocolid="TCPIP" value="127.0.0.1:7891" unique="FALSE"/>
	</ADDRESSES>
	<LINK_PROFILES>
		<LINK_PROFILE name="Fast Internet" requireExactMatch="TRUE">
			<LINK_REQS>
				<LINK_MINSPEED value="500000" />
				<LINK_REQ_PROTOCOL protocolid="TCPIP" />
			</LINK_REQS>
		</LINK_PROFILE>
  	</LINK_PROFILES>
</JCSP-CONFIG>
